{
  "name": "team3-library-web-app",
  "version": "1.0.0",
  "description": "A TypeScript Express API for managing books",
  "type": "module",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "node --loader ts-node/esm src/app.ts",
    "watch": "tsc --watch",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "check:unsafe": "biome check --write --unsafe .",
    "setup-hooks": "git config core.hooksPath .githooks"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "bcrypt": "^6.0.0",
    "express": "^4.18.2",
    "express-handlebars": "^8.0.1",
    "method-override": "^3.0.0",
    "sqlite3": "^5.1.7",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^4.17.17",
    "@types/express-handlebars": "^5.3.1",
    "@types/method-override": "^3.0.0",
    "@types/node": "^20.5.0",
    "@types/sqlite3": "^3.1.11",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "ts-node": {
    "esm": true
  },
  "keywords": [
    "express",
    "typescript",
    "api",
    "books"
  ],
  "author": "",
  "license": "ISC"
}
